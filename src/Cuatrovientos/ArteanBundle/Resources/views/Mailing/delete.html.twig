	<a href="{{ path('cuatrovientos_artean_mailing') }}" class="btn">
		<i class="fa fa-list-alt" aria-hidden="true"></i> Ver todos
	</a>

	<h3><i class="fa fa-trash-o" aria-hidden="true"></i> Eliminar mailing {{ mailing.id }}</h3>
 <div id='mailing'>
		<div>
			{% trans %}Est√°s segura{% endtrans %}
			<div class='title'>{{ mailing.subject }}</div>
