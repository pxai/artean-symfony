<div class="modal-dialog" role="document">
   <div class="modal-content">
      <form method="post" action="{{ path('cuatrovientos_artean_company_search') }}" class="arteanform">
      <div class="modal-header">

   <a href="{{ path('cuatrovientos_artean_company') }}" class="btn">
      <i class="fa fa-list-alt" aria-hidden="true"></i> Ver todas
   </a>

   <h3><i class="fa fa-search" aria-hidden="true"></i> Buscar empresa</h3>
         <h4 class="modal-title" id="exampleModalLabel">New message</h4>
      </div>
      <div class="modal-body">

   {{ form_errors(form) }}
   <div class="form-group">
      {{ form_label(form.cif) }}
      {{ form_errors(form.cif) }}
      {{ form_widget(form.cif, {'attr': {'class': 'form-control'}}) }}
   </div>
   <div class="form-group">
      {{ form_label(form.empresa) }}
      {{ form_errors(form.empresa) }}
      {{ form_widget(form.empresa, {'attr': {'class': 'form-control'}}) }}
   </div>

   <div class="form-group">
      {{ form_label(form.fct) }}
      {{ form_errors(form.fct) }}
      {{ form_widget(form.fct, {'attr': {'class': 'form-control'}}) }}
   </div>

   <div class="form-group">
      {{ form_label(form.convenio) }}
      {{ form_errors(form.convenio) }}
      {{ form_widget(form.convenio, {'attr': {'class': 'form-control'}}) }}
   </div>
      {{ form_row(form._token) }}

      </div>
      <div class="modal-footer">
         <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
         <button type="submit" class="btn btn-primary">Buscar empresas</button>
      </div>
      </form>
   </div>
</div>