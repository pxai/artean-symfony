{% extends "CuatrovientosArteanBundle:Default:layout.html.twig" %}
{% block title %}Artean :: Centers : Eliminar empresa{% endblock %}
{% block body %}
	<h2><i class="fa fa-university" aria-hidden="true"></i> Centro s de enseñanza</h2>

	<a href="{{ path('cuatrovientos_artean_center') }}" class="btn">
		<i class="fa fa-list-alt" aria-hidden="true"></i> Ver todos
	</a>

	<h3><i class="fa fa-trash-o" aria-hidden="true"></i> Eliminar centro de enseñanza {{ center.id }}</h3>
 <div id='center'>
		<div>
			{% trans %}Are you sure{% endtrans %}			
			<div class='title'>{{ center.name }}</div>
			<div><a href="{{ path('cuatrovientos_artean_center_delete_save',  {'id': center.id }) }}">Confirm deletion</a> </div>
			<div><a href="javascript:history.back()">Cancel</a> </div>
		</div>
</div>
{% endblock %}