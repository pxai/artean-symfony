{% extends "CuatrovientosArteanBundle:Default:layout.html.twig" %}
{% block title %}Artean :: Estudios : Eliminar estudios{% endblock %}
{% block body %}
	<h2><i class="fa fa-book" aria-hidden="true"></i> Estudios</h2>

	<a href="{{ path('cuatrovientos_artean_studies') }}" class="btn">
		<i class="fa fa-list-alt" aria-hidden="true"></i> Ver todos
	</a>

	<h3><i class="fa fa-trash-o" aria-hidden="true"></i> Eliminar estudios {{ studies.name }}</h3>
 <div id='studies'>
		<div>
			{% trans %}¿Estás seguro?{% endtrans %}
			<div class='title'>{{ studies.name }}</div>
			<div><a href="{{ path('cuatrovientos_artean_studies_delete_save',  {'id': studies.id }) }}">Confirm deletion</a> </div>
			<div><a href="javascript:history.back()">Cancel</a> </div>
		</div>
</div>
{% endblock %}