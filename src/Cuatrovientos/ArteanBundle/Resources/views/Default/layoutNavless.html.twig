<!DOCTYPE html>
<html>
<head>
<title>{% block title %}artean{% endblock title %}</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<!-- Bootstrap -->
{% stylesheets
    '@CuatrovientosArteanBundle/Resources/public/css/artean.css'
    '@CuatrovientosArteanBundle/Resources/public/css/bootstrap.min.css'
    '@CuatrovientosArteanBundle/Resources/public/css/bootstrap-theme.min.css'
    filter='cssrewrite' output='css/compiled/app.css' %}
    <link rel="stylesheet" type="text/css" href="{{ asset_url }}" >
{% endstylesheets %}
{% block extrastyles %}{% endblock %}

{% block head %}{% endblock %}

</head>

<body>
<div class="container">

    
{% block body %}{% endblock body %}

</div>
{% block javascripts %}
<!-- JavaScript plugins (requires jQuery) -->
<!--script src="//code.jquery.com/jquery.js"></script-->
<script src="https://use.fontawesome.com/09c458bfbf.js"></script>

        <!-- Include all compiled plugins (below), or include individual files as needed -->

     {% javascripts
     '@CuatrovientosArteanBundle/Resources/public/js/jquery.min.js'
     '@CuatrovientosArteanBundle/Resources/public/js/tether.min.js'
     '@CuatrovientosArteanBundle/Resources/public/js/bootstrap.min.js'
     %}
        <!-- Optionally enable responsive features in IE8 -->
        <script src="{{ asset_url }}"></script>
     {% endjavascripts %}

    
    {# % javascripts '@CuatrovientosArteanBundle/Resources/public/js/respond.js' % }
        <!-- Optionally enable responsive features in IE8 -->
        <script src="{{ asset_url }}"></script> 
     {  %endjavascripts % #}
{% endblock %}
{% block extrascripts %}{% endblock %}
</body>
</html>