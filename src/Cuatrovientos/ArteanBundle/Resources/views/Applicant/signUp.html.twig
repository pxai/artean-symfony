
{# src/Cuatrovientos/ArteanBundle/Resources/views/Center/new.html.twig #}
{% extends "CuatrovientosArteanBundle:Default:layoutStars.html.twig" %}
{% block title %}Artean :: Alta de candidato {% endblock %}
{% block body %}

<h2>Alta en Bolsa de empleo</h2>
                   {% image '@CuatrovientosArteanBundle/Resources/public/img/alien.png' %}
                        <img src="{{ asset_url }}"  alt="darest candidates" title="fearless candidate"/>
                    {% endimage %}
<p>Tu email no existe en nuestra BBDD.</p>
<form method="post" action="{{ path('cuatrovientos_artean_applicant_signup_save') }}">
   <div class="form-group">
        {{ form_label(form.name) }}
        {{ form_errors(form.name) }}
        {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
    </div>
   <div class="form-group">
        {{ form_label(form.surname) }}
        {{ form_errors(form.surname) }}
        {{ form_widget(form.surname, {'attr': {'class': 'form-control'}}) }}
    </div>
   <div class="form-group">
        {{ form_label(form.email) }}
        {{ form_errors(form.email) }}
        {{ form_widget(form.email, {'attr': {'class': 'form-control'}}) }}
    </div>
   <div class="form-group">
        {{ form_label(form.mobile) }}
        {{ form_errors(form.mobile) }}
        {{ form_widget(form.mobile, {'attr': {'class': 'form-control'}}) }}
    </div>
   <div class="form-group">
        {{ form_label(form.studies) }}
        {{ form_errors(form.studies) }}
        {{ form_widget(form.studies, {'attr': {'class': 'form-control checkboxblock'}}) }}
    </div>
   <div class="form-group">
        {{ form_label(form.lopd) }}
        {{ form_errors(form.lopd) }}
        {{ form_widget(form.lopd, {'attr': {'class': 'form-control'}}) }}
        <button type="button" class="btn btn-info-outline btn-xs" data-toggle="modal" data-target="#lopd-modal">
  Ver declaración de privacidad
</button>

    </div>
    <div class="form-group">
        {{ form_row(form._token) }}
        {{ form_widget(form.save, {'attr': {'class': 'form-control'}}) }}
    </div>
</form>
            
<div class="modal fade" id="lopd-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
<div  class="cerrar">
        <button type="button" class="btn btn-info-outline btn-xs" data-toggle="modal" data-target="#lopd-modal">
                                               Cerrar</button>
</div>
    
